        "channelgrabber/location": "~1.1.3",
        "channelgrabber/locking": "~1.1.0",
        "channelgrabber/log": "~2.0.27",
        "channelgrabber/middleware": "~1.0.3",
        "channelgrabber/notification": "~1.0.0",
        "channelgrabber/order": "dev-CGIV-7796-CGAppPhp71 as 1.24.5",

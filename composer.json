{
    "repositories": [
        {
            "url": "https://composer.build.channelgrabber.com",
            "type": "composer",
            "options": {
                "ssl": {
                    "local_cert": "composer.pem"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.zendframework.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CG\\": "library/CG/",
            "CG\\Controllers\\": "controllers/CG/Controllers/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CG\\Order\\Test\\": "tests/unit/CG/Order/Test",
            "CG\\SequentialNumbering\\Test\\": "tests/unit/CG/SequentialNumbering/Test"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "require": {
        "alcaeus/mongo-php-adapter": "~1.0.6",
        "aws/aws-sdk-php": "^3.19",
        "channelgrabber/amazon": "~1.24.0",
        "channelgrabber/bigcommerce": "~1.0.5",
        "channelgrabber/cache": "~2.4.1",
        "channelgrabber/cglib": "~1.13.1",
        "channelgrabber/channel": "~1.32.6",
        "channelgrabber/cilex": "~1.1.4",
        "channelgrabber/constants": "dev-master",
        "channelgrabber/dataplug": "~1.2.1",
        "channelgrabber/db": "~1.0.9",
        "channelgrabber/di": "~2.2.1",
        "channelgrabber/ebay": "~1.9.46",
        "channelgrabber/ekm": "~1.9.2",
        "channelgrabber/email": "~1.1.1",
        "channelgrabber/etag": "~1.6.0",
        "channelgrabber/exchangerate": "~1.1.1",
        "channelgrabber/gearman": "~1.2.29",
        "channelgrabber/http": "~1.3.31",
        "channelgrabber/image": "~1.2.7",
        "channelgrabber/listing": "~1.11.13",
        "channelgrabber/locale": "~1.0.5",
        "channelgrabber/location": "~1.2.0",
        "channelgrabber/locking": "~1.1.0",
        "channelgrabber/log": "~2.1.2",
        "channelgrabber/middleware": "~1.0.4",
        "channelgrabber/notification": "~1.2.0",
        "channelgrabber/order": "~1.29.4",
        "channelgrabber/phinx": "~0.3.8",
        "channelgrabber/predis": "~1.0.13",
        "channelgrabber/product": "dev-CGIV-8910-DeleteProductShouldDeleteMerchantStockLocations as 1.9.6",
        "channelgrabber/product-locking": "~1.0.0",
        "channelgrabber/purchase-order": "~1.0.0",
        "channelgrabber/purge": "~1.0.5",
        "channelgrabber/redis": "~1.0.1",
        "channelgrabber/redis-locking": "~1.0.0",
        "channelgrabber/sequential-numbering": "~1.0.1",
        "channelgrabber/settings": "~1.5.3",
        "channelgrabber/shopify": "~1.0.0",
        "channelgrabber/slim": "~1.13.30",
        "channelgrabber/spork": "~0.2.1",
        "channelgrabber/stats": "~1.0.3",
        "channelgrabber/stdlib": "~2.13.2",
        "channelgrabber/stock": "~1.10.1",
        "channelgrabber/stock-locking": "~1.1.0",
        "channelgrabber/tax": "~1.0.2",
        "channelgrabber/template": "~1.4.13",
        "channelgrabber/transaction": "~1.2.1",
        "channelgrabber/usage": "~1.0.7",
        "channelgrabber/user": "~1.23.0",
        "channelgrabber/userpreference": "~1.2.5",
        "channelgrabber/validation": "~1.2.13",
        "channelgrabber/vnderror": "~1.0.0",
        "channelgrabber/woocommerce": "~1.2.0",
        "channelgrabber/zf2-stdlib": "~1.2.0",
        "cilex/cilex": "~1.0.1",
        "guzzle/guzzle": "~3.7",
        "guzzlehttp/guzzle": "^5.3.1",
        "nocarrier/hal": "~0.9.10",
        "phpish/link_header": "@dev",
        "predis/predis": "~0.8.7",
        "realityking/hash_equals": "@dev",
        "slim/slim": "~2.6.3",
        "zendframework/zend-config": "2.4.0",
        "zendframework/zend-db": "2.4.0",
        "zendframework/zend-di": "2.4.0",
        "zendframework/zend-eventmanager": "2.4.0",
        "zendframework/zend-inputfilter": "2.4.0",
        "zendframework/zend-servicemanager": "2.4.0",
        "zendframework/zend-validator": "2.4.0"
    },
    "require-dev": {
        "channelgrabber/codeception": "~1.2.1",
        "channelgrabber/phpunit": "~1.0.0",
        "codeception/codeception": "~1.8.0",
        "phpspec/phpspec": "2.0.*@dev",
        "phpspec/prophecy": "~1.0"
    },
    "scripts": {
        "post-install-cmd": "@setup",
        "post-update-cmd": "@setup",
        "setup": [
            "@composer -o dump-autoload"
        ]
    }
}
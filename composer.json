{
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/channelgrabber/Component_ZendDb.git"
        },
        {
            "url": "https://composer.build.channelgrabber.com",
            "type": "composer",
            "options": {
                "ssl": {
                    "local_cert": "composer.pem"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.zendframework.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CG\\": "library/CG/",
            "CG\\Controllers\\": "controllers/CG/Controllers/"

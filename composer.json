{
    "repositories": [
        {
            "url": "https://composer.build.channelgrabber.com",
            "type": "composer",
            "options": {
                "ssl": {
                    "local_cert": "composer.pem"
                }
            }
        },
        {
            "type": "composer",
            "url": "http://packages.zendframework.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CG\\": "library/CG/",
            "CG\\Controllers\\": "controllers/CG/Controllers/"
        }
    },
    "require": {
        "channelgrabber/cilex": "~1.1.0",
        "channelgrabber/constants": "dev-master",
        "channelgrabber/di": "~2.1.1",
        "channelgrabber/http": "~1.2.28",
        "channelgrabber/log": "~2.0.9",
        "channelgrabber/middleware": "dev-CGIV-4564-MiddlewareHandler",
        "channelgrabber/slim": "~1.12.0",
        "channelgrabber/stdlib": "~2.7.16",
        "channelgrabber/usage": "~1.0.0",
        "channelgrabber/user": "~1.14.3",
        "channelgrabber/validation": "~1.2.0",
        "channelgrabber/vnderror": "~1.0.0",
        "channelgrabber/zf2-stdlib": "~1.1.14",
        "composer/composer": "@dev",
        "guzzle/guzzle": "~3.7",
        "nocarrier/hal": "~0.9.3",
        "php-jsonpatch/php-jsonpatch": "@dev",
        "php-jsonpointer/php-jsonpointer": "@dev",
        "robmorgan/phinx": "v0.2.8",
        "slim/slim": "2.3.*",
        "zendframework/zend-config": "~2.2.5",
        "zendframework/zend-db": "~2.2.5",
        "zendframework/zend-di": "~2.2.5",
        "zendframework/zend-eventmanager": "~2.2.5",
        "zendframework/zend-inputfilter": "~2.2.5",
        "zendframework/zend-servicemanager": "~2.2.5",
        "zendframework/zend-validator": "~2.2.5"
    },
    "require-dev": {
        "channelgrabber/codeception": "~1.1.47",
        "codeception/codeception": "1.8.0",
        "phpspec/phpspec": "2.0.*@dev",
        "phpspec/prophecy": "~1.0"
    },
    "scripts": {
        "post-install-cmd": [
            "composer -o dump-autoload"
        ],
        "post-update-cmd": [
            "composer -o dump-autoload"
        ]
    }
}

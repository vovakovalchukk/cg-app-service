            "CG\\Order\\Test\\": "tests/unit/CG/Order/Test",
            "CG\\SequentialNumbering\\Test\\": "tests/unit/CG/SequentialNumbering/Test"
        }
    },
    "config": {
        "preferred-install": "dist"
    },

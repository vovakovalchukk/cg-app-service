{
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/channelgrabber/Component_ZendDb.git"
        },
        {
            "url": "https://composer.build.channelgrabber.com",
            "type": "composer",
            "options": {
                "ssl": {
                    "local_cert": "composer.pem"
                }
            }
        },
        {
            "type": "composer",
            "url": "http://packages.zendframework.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CG\\": "library/CG/",
            "CG\\Controllers\\": "controllers/CG/Controllers/"
        }
    },
    "require": {
        "channelgrabber/amazon": "~1.6.20",
        "channelgrabber/cache": "dev-CGIV-4085-cache-invalidate-embeds as 2.0.1",
        "channelgrabber/cglib": "~1.1.7",
        "channelgrabber/channel": "~1.11.5",
        "channelgrabber/cilex": "~1.0.3",
        "channelgrabber/constants": "dev-master",
        "channelgrabber/di": "~2.1.1",
        "channelgrabber/ebay": "~1.4.6",
        "channelgrabber/ekm": "~1.0.13",
        "channelgrabber/etag": "~1.5.0",
        "channelgrabber/gearman": "~1.1.0",
        "channelgrabber/http": "~1.2.14",
        "channelgrabber/image": "~1.1.2",
        "channelgrabber/listing": "~1.2.10",
        "channelgrabber/location": "~1.1.1",
        "channelgrabber/log": "~2.0.6",
        "channelgrabber/order": "~1.9.6",
        "channelgrabber/phinx": "~0.3.2.2",
        "channelgrabber/predis": "~1.0.3",
        "channelgrabber/product": "~1.3.14",
        "channelgrabber/settings": "~1.3.0",
        "channelgrabber/slim": "~1.11.2",
        "channelgrabber/stdlib": "~2.7.28",
        "channelgrabber/stock": "~1.3.4",
        "channelgrabber/template": "~1.2.36",
        "channelgrabber/transaction": "~1.1.3",
        "channelgrabber/usage": "~1.0.5",
        "channelgrabber/user": "~1.18.7",
        "channelgrabber/userpreference": "~1.2.2",
        "channelgrabber/validation": "~1.2.5",
        "channelgrabber/vnderror": "~1.0.0",
        "channelgrabber/zf2-stdlib": "~1.1.17",
        "composer/composer": "@dev",
        "guzzle/guzzle": "~3.7",
        "nocarrier/hal": "~0.9.4",
        "php-jsonpatch/php-jsonpatch": "@dev",
        "php-jsonpointer/php-jsonpointer": "@dev",
        "predis/predis": "~0.8.3",
        "slim/slim": "2.3.*",
        "zendframework/zend-config": "2.3.*",
        "zendframework/zend-db": "~2.3.1",
        "zendframework/zend-di": "2.3.*",
        "zendframework/zend-eventmanager": "2.3.*",
        "zendframework/zend-inputfilter": "2.3.*",
        "zendframework/zend-servicemanager": "2.3.*",
        "zendframework/zend-validator": "2.3.*"
    },
    "require-dev": {
        "channelgrabber/codeception": "~1.2.0",
        "channelgrabber/phpunit": "~1.0.0",
        "codeception/codeception": "~1.8.0",
        "phpspec/phpspec": "2.0.*@dev",
        "phpspec/prophecy": "~1.0"
    },
    "scripts": {
        "post-install-cmd": [
            "composer -o dump-autoload"
        ],
        "post-update-cmd": [
            "composer -o dump-autoload"
        ]
    }
}